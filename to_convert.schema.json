{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "to_convert",
  "type": "object",
  "patternProperties": {
    ".*": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "converter",
          "converter_type",
          "outputs"
        ],
        "properties": {
          "description": {
            "type": "string"
          },
          "converter": {
            "type": "string"
          },
          "converter_type": {
            "type": "string",
            "enum": [
              "master",
              "statutory",
              "access"
            ]
          },
          "outputs": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "minItems": 1
          }
        }
      }
    }
  }
}